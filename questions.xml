    <choices>
      <choice>Edward Nygma</choice>
      <choice>Harvey Dent</choice>
      <choice correct="yes">Oswald Cobblepot</choice>
      <choice>Victor Zsasz</choice>
    </choices>
  </question>

  <question id="20">
    <wording>What is the name of the group Batman is part of?</wording>
    <choices>
