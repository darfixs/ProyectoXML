<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE test SYSTEM "archivo.dtd">
<test>
  <question id="1">
    <wording>What is Batman's real name?</wording>
    <choices>
      <choice correct="yes">Bruce Wayne</choice>
      <choice>Clark Kent</choice>
      <choice>Peter Parker</choice>
      <choice>Stephen Strange</choice>
    </choices>
  </question>

  <question id="2">
    <wording>Which city does Batman protect?</wording>
    <choices>
      <choice correct="yes">Gotham</choice>
      <choice>Metropolis</choice>
      <choice>Ciudad Gótica</choice>
      <choice>Central City</choice>
    </choices>
  </question>

  <question id="3">
    <wording>What is the name of Batman's butler?</wording>
    <choices>
      <choice>James</choice>
      <choice correct="yes">Alfred</choice>
      <choice>Jarvis</choice>
      <choice>Lucius</choice>
    </choices>
  </question>

  <question id="4">
    <wording>Who is Batman's most famous enemy?</wording>
    <choices>
      <choice>Lex Luthor</choice>
      <choice>Green Goblin</choice>
      <choice>Thanos</choice>
      <choice correct="yes">The Joker</choice>
    </choices>
  </question>

  <question id="5">
    <wording>What traumatic event motivated Bruce Wayne to become Batman?</wording>
    <choices>
      <choice correct="yes">The death of his parents</choice>
      <choice>An explosion</choice>
      <choice>A lab accident</choice>
      <choice>An alien invasion</choice>
    </choices>
  </question>

  <question id="6">
    <wording>What is Batman's main vehicle?</wording>
    <choices>
      <choice>Batbike</choice>
      <choice>Invisible Car</choice>
      <choice correct="yes">Batmobile</choice>
      <choice>Supercar</choice>
    </choices>
  </question>

  <question id="7">
    <wording>What alias does Dick Grayson use after being Robin?</wording>
    <choices>
      <choice>Red Hood</choice>
      <choice>Batboy</choice>
      <choice>Night Falcon</choice>
      <choice correct="yes">Nightwing</choice>
    </choices>
  </question>

  <question id="8">
    <wording>What is the name of the prison where Batman locks up many villains?</wording>
    <choices>
      <choice>Blackgate</choice>
     <choice correct="yes">Arkham Asylum</choice>
      <choice>Iron Heights</choice>
      <choice>Rikers Island</choice>
    </choices>
  </question>

  <question id="9">
    <wording>Who created the character Batman?</wording>
    <choices>
      <choice correct="yes">Bob Kane and Bill Finger</choice>
      <choice>Stan Lee</choice>
      <choice>Steve Ditko</choice>
      <choice>Jack Kirby</choice>
    </choices>
  </question>

  <question id="10">
    <wording>What is the name of Bruce Wayne's company?</wording>
    <choices>
      <choice>LexCorp</choice>
      <choice correct="yes">Wayne Enterprises</choice>
      <choice>Stark Industries</choice>
      <choice>Oscorp</choice>
    </choices>
  </question>

  <question id="11">
    <wording>Who is the commissioner allied with Batman?</wording>
    <choices>
      <choice>Harvey Dent</choice>
      <choice>John Blake</choice>
      <choice correct="yes">Jim Gordon</choice>
      <choice>Ben Urich</choice>
    </choices>
  </question>

  <question id="12">
    <wording>What animal inspired Batman's costume?</wording>
    <choices>
      <choice correct="yes">Bat</choice>
      <choice>Lion</choice>
      <choice>Wolf</choice>
      <choice>Eagle</choice>
